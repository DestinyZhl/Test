@section ViewScripts{
    @* ReSharper disable once UsageOfPossiblyUnassignedValue *@
    <script type="text/javascript">
        $(function () {
            $("#Dropdown_Report li").click(function () {
                $("#Dropdown_Report li").find("input").each(function () {
                    var checkedId = this.id;
                    checkedId = checkedId.substring(4, checkedId.length);
                    if (this.checked == true) {
                        $("#" + checkedId).show();
                    } else
                        $("#" + checkedId).hide();
                });
            });
            $("#js_my_task li").click(function () {
                $("#js_my_task li").each(function () {
                    if (this.className == "active") {
                        this.className = "";
                    }
                });
            });
        });
    </script>
}

